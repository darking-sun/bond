<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.bond.mapper.CustomerLimitMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dev.bond.entity.CustomerLimit">
        <id column="ACCOUNT_ID" property="accountId" />
        <result column="TOTAL_SHARE" property="totalShare" />
        <result column="BUY_SHARE" property="buyShare" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ACCOUNT_ID AS accountId, TOTAL_SHARE AS totalShare, BUY_SHARE AS buyShare
    </sql>

</mapper>
